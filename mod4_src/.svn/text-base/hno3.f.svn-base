      SUBROUTINE HNO3 (V,HABS)

!     HNO3  STATISTICAL BAND PARAMETERS

      DIMENSION H1(15), H2(16), H3(13)
!     ARRAY H1 CONTAINS HNO3 ABS, COEF(CM-1ATM-1) FROM  850 TO 920 CM-1
      DATA H1/2.197,3.911,6.154,8.150,9.217,9.461,11.56,11.10,11.17,12.4&
     &0,10.49,7.509,6.136,4.899,2.866/
!     ARRAY H2 CONTAINS HNO3 ABS, COEF(CM-1ATM-1) FROM 1275 TO1350 CM-1
      DATA H2/2.828,4.611,6.755,8.759,10.51,13.74,18.00,21.51,23.09,21.6&
     &8,21.32,16.82,16.42,17.87,14.86,8.716/
!     ARRAY H3 CONTAINS HNO3 ABS, COEF(CM-1ATM-1) FROM 1675 TO1735 CM-1
      DATA H3/5.003,8.803,14.12,19.83,23.31,23.58,23.22,21.09,26.99,25.8&
     &4,24.79,17.68,9.420/
      HABS=0.
      IF (V.GE.850.0.AND.V.LE.920.0) GO TO 5
      IF (V.GE.1275.0.AND.V.LE.1350.0) GO TO 10
      IF (V.GE.1675.0.AND.V.LE.1735.0) GO TO 15
      RETURN
    5 I=INT((V-845.)/5.)
      HABS=H1(I)
      RETURN
   10 I=INT((V-1270.)/5.)
      HABS=H2(I)
      RETURN
   15 I=INT((V-1670.)/5.)
      HABS=H3(I)
      RETURN
      END
